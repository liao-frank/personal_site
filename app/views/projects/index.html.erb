<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Subtitle</th>
<!--       <th>Snippet</th>
      <th>Description</th> -->
      <th>Dates</th>
      <th>Collaboration</th>
      <th>Cover url</th>
      <th>Order</th>
      <th>Visibility</th>
      <th>Site link</th>
      <th>Github link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.subtitle %></td>
<!--         <td><%= project.snippet %></td>
        <td><%= project.description %></td> -->
        <td><%= project.dates %></td>
        <td><%= project.collaboration %></td>
        <td><%= project.cover_url %></td>
        <td><%= project.order %></td>
        <td><%= project.visibility %></td>
        <td><%= project.site_link %></td>
        <td><%= project.github_link %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>